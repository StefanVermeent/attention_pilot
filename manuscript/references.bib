	month = jul,
	year = {2010},
	keywords = {Attention, Culture differences, Global–local processing, Race differences},
	pages = {1540--1549},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\Stefa\\Zotero\\storage\\DKSJXPHZ\\McKone et al. - 2010 - Asia has the global advantage Race and visual att.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Stefa\\Zotero\\storage\\KQ3ANZY9\\S0042698910002312.html:text/html},

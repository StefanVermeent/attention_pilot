@article{simonsohn_2020,
	title = {Specification curve analysis},
	volume = {4},
	copyright = {2020 The Author(s), under exclusive licence to Springer Nature Limited},
	issn = {2397-3374},
	url = {https://www.nature.com/articles/s41562-020-0912-z},

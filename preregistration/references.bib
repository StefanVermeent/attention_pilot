@article{lyon_reframing_2021,
	title = {Reframing cognition: getting down to biological basics},
	volume = {376},
	issn = {0962-8436, 1471-2970},
	shorttitle = {Reframing cognition},
	url = {https://royalsocietypublishing.org/doi/10.1098/rstb.2019.0750},
	doi = {10.1098/rstb.2019.0750},
	abstract = {The premise of this two-part theme issue is simple: the cognitive sciences should join the rest of the life sciences in how they approach the quarry within their research domain. Specifically, understanding how organisms on the lower branches of the phylogenetic tree become familiar with, value and exploit elements of an ecological niche while avoiding harm can be expected to aid understanding of how organisms that evolved later (including
              Homo sapiens
              ) do the same or similar things. We call this approach basal cognition. In this introductory essay, we explain what the approach involves. Because no definition of cognition exists that reflects its biological basis, we advance a working definition that can be operationalized; introduce a behaviour-generating toolkit of capacities that comprise the function (e.g. sensing/perception, memory, valence, learning, decision making, communication), each element of which can be studied relatively independently; and identify a (necessarily incomplete) suite of common biophysical mechanisms found throughout the domains of life involved in implementing the toolkit. The articles in this collection illuminate different aspects of basal cognition across different forms of biological organization, from prokaryotes and single-celled eukaryotes—the focus of Part 1—to plants and finally to animals, without and with nervous systems, the focus of Part 2. By showcasing work in diverse, currently disconnected fields, we hope to sketch the outline of a new multidisciplinary approach for comprehending cognition, arguably the most fascinating and hard-to-fathom evolved function on this planet. Doing so has the potential to shed light on problems in a wide variety of research domains, including microbiology, immunology, zoology, biophysics, botany, developmental biology, neurobiology/science, regenerative medicine, computational biology, artificial life and synthetic bioengineering.
            
            This article is part of the theme issue ‘Basal cognition: conceptual tools and the view from the single cell’.},
	language = {en},
	number = {1820},
	urldate = {2021-01-25},
	journal = {Philosophical Transactions of the Royal Society B: Biological Sciences},
	author = {Lyon, Pamela and Keijzer, Fred and Arendt, Detlev and Levin, Michael},
	month = mar,
	year = {2021},
	pages = {20190750},
	file = {Lyon et al. - 2021 - Reframing cognition getting down to biological ba.pdf:C\:\\Users\\Stefa\\Zotero\\storage\\XJZDVBZK\\Lyon et al. - 2021 - Reframing cognition getting down to biological ba.pdf:application/pdf},
}

@article{semin_socially_2013,
	title = {Socially {Situated} {Cognition} in {Perspective}},
	volume = {31},
	issn = {0278-016X},
	url = {http://guilfordjournals.com/doi/abs/10.1521/soco.2013.31.2.125},
	doi = {10.1521/soco.2013.31.2.125},
	abstract = {In 2004, we (Smith \& Semin, 2004) described a conceptual framework of “socially situated cognition,” encompassing four major themes. Cognition is for adaptive action, involves the body and sensori-motor systems, is situated in immediate intercourse with the environment, and is distributed across other minds and tools. Here, we introduce two broader themes: social cognition is special because other people's movements and other characteristics can be mapped onto our own bodies; and social cognition is emergent, influencing the parts and subsystems that generate it rather than the reverse causal direction. We then review the current state of theory and research in the four topic areas that we laid out in 2004. We conclude by noting that much research on these themes has occurred outside of social psychology, and stressing the benefits for the future of an integrative, interdisciplinary approach to these core issues of social psychology.},
	number = {2},
	urldate = {2021-01-22},
	journal = {Social Cognition},
	author = {Semin, Gün R. and Smith, Eliot R.},
	month = apr,
	year = {2013},
	note = {Publisher: Guilford Publications Inc.},
	pages = {125--146},
	file = {Full Text PDF:C\:\\Users\\Stefa\\Zotero\\storage\\WYRFDY72\\Semin and Smith - 2013 - Socially Situated Cognition in Perspective.pdf:application/pdf},
}

@article{pritchard_situated_2018,
	title = {Situated cognition and the function of behavior},
	volume = {13},
	issn = {1911-4745(Electronic)},
	doi = {10.3819/CCBR.2018.130005},
	abstract = {In his review of “situated cognition” Cheng (see record 2018-58833-001) reminds us that the properties of cognition can be influenced by much more than what is going on in the brain. In this commentary, the author focus on the lessons that this situated approach can teach those of us using behavior as a tool for investigating animal cognition. Rather than just a measure telling us about hidden cognitive processes, the details of behavior can provide important clues about how animals are solving a task. By looking in more detail at the behavior of our animals, and the possible sensory consequences of these behaviors, we can not only learn more about how animals do what they need to do but also explore how situated cognition shapes the structure of behavior. (PsycINFO Database Record (c) 2019 APA, all rights reserved)},
	journal = {Comparative Cognition \& Behavior Reviews},
	author = {Pritchard, David J.},
	year = {2018},
	note = {Place: US
Publisher: The Comparative Cognition Society},
	keywords = {Animal Cognition, Animal Ethology, Central Nervous System},
	pages = {34--39},
	file = {Submitted Version:C\:\\Users\\Stefa\\Zotero\\storage\\HRWKQ54V\\Pritchard - 2018 - Situated cognition and the function of behavior.pdf:application/pdf;Snapshot:C\:\\Users\\Stefa\\Zotero\\storage\\MC2ZXUGE\\2018-58833-005.html:text/html},
}

@article{cheng_cognition_2018,
	title = {Cognition beyond representation: {Varieties} of situated cognition in animals},
	volume = {13},
	issn = {1911-4745(Electronic)},
	shorttitle = {Cognition beyond representation},
	doi = {10.3819/CCBR.2018.130001},
	abstract = {The notion that cognition comprises more than computations of a central nervous system operating on representations has gained a foothold in human cognitive science for a few decades now. Various brands of embodied, extended, enacted, and distributed cognition, some more conservative and some more liberal, have paraded in philosophy and cognitive science. I call the genus including all such species situated cognition and go on to depict selected cases in nonhuman comparative cognition. Distributed cognition is often used as another term for situated cognition. But behavioral biologists have used the term in another sense, to mean the reduction of cognitive capacities arising from team work in cooperative societies. Hymenopteran insects have been studied as cases. The octopus displays embodied cognition, with some of the computational work offloaded to the periphery. Web-building spiders showcase extended cognition, in which objects external to the animal—the web, in the case of spiders—play a crucial causal role in cognition. A criterion of mutual manipulability, in which causal influence flows both ways between organism and extended object, serves to delimit the scope of extended cognition. Play in dogs features intelligence on-the-run, arising out of action, a key characteristic of enactive cognition. I discuss other cases in which action entwines with central representational cognition to achieve goal-directed behavior. Considering situated cognition in diverse animals leads to myriad research questions that can enrich the field. (PsycINFO Database Record (c) 2019 APA, all rights reserved)},
	journal = {Comparative Cognition \& Behavior Reviews},
	author = {Cheng, Ken},
	year = {2018},
	note = {Place: US
Publisher: The Comparative Cognition Society},
	keywords = {Animal Cognition, Animal Ethology, Central Nervous System},
	pages = {1--20},
	file = {Full Text:C\:\\Users\\Stefa\\Zotero\\storage\\NQ6RJD36\\Cheng - 2018 - Cognition beyond representation Varieties of situ.pdf:application/pdf;Snapshot:C\:\\Users\\Stefa\\Zotero\\storage\\Z5QQLAHV\\2018-58833-001.html:text/html},
}

@article{wright_contextual_1991,
	title = {Contextual dependencies in motor skills},
	volume = {19},
	issn = {1532-5946},
	url = {https://doi.org/10.3758/BF03197140},
	doi = {10.3758/BF03197140},
	abstract = {The development of contextual dependencies during motor skill acquisition was examined. Environmental context was varied along intentional and incidental dimensions. Intentional stimuli were defined as essential for achieving skilled performance, whereas incidental stimuli were defined as those that have the potential to become associated with specific tasks due to their selective presence in the learning environment. Experiment 1 demonstrated the occurrence of contextual dependencies for the learning of four-key typing sequences. Contextual dependencies were diminished in Experiment 2 when the number of keys used in the sequences was reduced. In Experiment 3, a retention condition was incorporated, in which both the intentional and the incidental stimuli were not available; this confirmed that task difficulty mediated the development of contextual dependencies. These findings are discussed with respect to the incorporation of environmental contextual stimuli with memorial representations of movement information.},
	language = {en},
	number = {4},
	urldate = {2021-01-22},
	journal = {Memory \& Cognition},
	author = {Wright, David L. and Shea, Charles H.},

	author = {Gehred, Maria Z. and Knodt, Annchen R. and Ambler, Antony and Bourassa, Kyle J. and Danese, Andrea and Elliott, Maxwell L. and Hogan, Sean and Ireland, David and Poulton, Richie and Ramrakha, Sandhya and Reuben, Aaron and Sison, Maria L. and Moffitt, Terrie E. and Hariri, Ahmad R. and Caspi, Avshalom},
	month = mar,

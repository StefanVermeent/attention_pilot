@article{he_role_nodate,
	title = {The {Role} of {Working} {Memory} {Capacity} in {Spatial} {Learning} {Depends} on {Spatial} {Information} {Integration} {Difficulty} in the {Environment}},
	issn = {0096-3445},
	doi = {10.1037/xge0000972},
	abstract = {A substantial amount of research has been conducted to uncover factors underlying the pronounced individual differences in spatial navigation. Spatial working memory capacity (SWM) is shown to be one important factor. In other domains such as reading comprehension, the role of working memory capacity in task performance differences depends on the difficulty of other task demands. In the current study, we investigated whether, similarly, the relationship between SWM and spatial performance was dependent on the difficulty of spatial information integration in the environment. Based on our prior work, spatial information integration difficulty depends on (a) difficulty in observing spatial relationships between locations of interest in the environment and (b) the individual's ability to integrate such relationships. Leveraging virtual reality, we manipulated the difficulty in observing the spatial relationships during learning by changing the visibility of the buildings, and measured individual's self-report sense of direction (SOD) which modulates the ability to integrate such relationships under different degrees of visibility. We consistently found that in the "easy" spatial integration condition (high SOD with high visibility), high SWM did not significantly improve spatial learning. The same pattern was observed in the difficult condition (low SOD with low visibility). On the other hand, high SWM improved spatial learning for medium difficulty (high SOD with low visibility, or vice versa). Together, our results reveal that the role of SWM in spatial learning differences depends on spatial integration difficulty. Our results also have significant applied implications for using virtual reality to target and facilitate spatial learning., (C) 2021 by the American Psychological Association},
	language = {English.},
	journal = {Journal of Experimental Psychology},
	author = {He, Qiliang and Han, Andrew T. and Churaman, Tanya A. and Brown, Thackery I.},
	note = {Institution: School of Psychology, Georgia Institute of Technology, School of Computer Science, Georgia Institute of Technology, School of Psychology, Georgia Institute of Technology},
	keywords = {individual differences, working memory, cognitive map, spatial navigation, virtual reality},
}


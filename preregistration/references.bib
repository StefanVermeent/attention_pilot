	keywords = {stress, development, executive function, parenting, specialization},
	file = {Full Text PDF:C\:\\Users\\Stefa\\Zotero\\storage\\UYC3LZWN\\Fields et al. - Adaptation in the face of adversity Decrements an.pdf:application/pdf;Snapshot:C\:\\Users\\Stefa\\Zotero\\storage\\9EHP4ARL\\desc.html:text/html},

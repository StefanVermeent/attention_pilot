	file = {Ellis et al. - 2020 - Hidden talents in harsh environments.pdf:C\:\\Users\\Stefa\\Zotero\\storage\\Q7ZZP5NQ\\Ellis et al. - 2020 - Hidden talents in harsh environments.pdf:application/pdf},
}


@article{wysocki_statistical_2020,
	title = {Statistical {Control} {Requires} {Causal} {Justification}},
	url = {https://psyarxiv.com/j9vw4/},
	abstract = {It is common practice in psychological research to use statistical control to remove the effect of third variables in correlational or quasi-experimental studies. Controlling for relevant confounders can improve estimates of the causal path from a predictor to the outcome, but this only works under ideal circumstances. When the selected control variables are inappropriate, controlling can result in regression coefficient estimates that are further from the causal path than uncontrolled estimates. Despite the ubiquity of control variables in published regression analyses and the consequences of controlling for inappropriate third variables, the selection of control variables is rarely well justified. We argue that to carefully select appropriate control variables researchers must propose and defend a causal structure that includes the outcome, predictors, and plausible confounders. Throughout, we underscore the importance of causality when selecting control variables by demonstrating via simulation the consequence of controlling for inappropriate variables. Finally, we provide practical recommendations for applied researchers who wish to use statistical control.},
	urldate = {2021-02-12},
	author = {Wysocki, Anna and Lawson, Katherine M. and Rhemtulla, Mijke},
	month = oct,
	year = {2020},
	doi = {10.31234/osf.io/j9vw4},
	keywords = {Social and Behavioral Sciences, Quantitative Methods, Causality, Collider, Confounder, Incremental Validity, Mediator, Quantitative Psychology, Statistical Control},
	file = {Full Text PDF:C\:\\Users\\Stefa\\Zotero\\storage\\PGYFFKWI\\Wysocki et al. - 2020 - Statistical Control Requires Causal Justification.pdf:application/pdf},
}

@article{raab_detection_2020,
	title = {Detection of environmental controllability improves across development},
	url = {https://psyarxiv.com/xdt9p/},
	abstract = {Accurate assessment of the controllability of the environment enables individuals to adaptively adjust their behavior—exploiting rewards when desirable outcomes are contingent upon their actions and minimizing costly deliberation about how to behave when their actions are inconsequential. However, it remains unclear how inferences of environmental controllability change from childhood to adulthood. Using a task that covertly alternated between a controllable and uncontrollable condition, we found that accurate controllability assessments improved with age and that participants of all ages exhibited a bias toward beliefs of controllability. Computational modeling revealed a qualitative shift across development in the learning processes underlying task performance, with the use of task structure knowledge to guide informative interventions that revealed the degree of environmental controllability emerging as the predominant strategy during adolescence. Age-related improvements in the detection of environmental controllability may foster increasingly adaptive behavior into adulthood by revealing when actions can be leveraged for one’s benefit.},
	urldate = {2021-02-11},
	author = {Raab, Hillary and Foord, Careen and Ligneul, Romain and Hartley, Catherine},
	month = nov,

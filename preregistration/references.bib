	author = {Verbruggen, Frederick and McLaren, Ian P. L. and Chambers, Christopher D.},
	month = sep,
	year = {2014},
	note = {Publisher: SAGE Publications Inc},
	keywords = {executive control, learning, action control, behavior change},
	pages = {497--524},
	file = {SAGE PDF Full Text:C\:\\Users\\Stefa\\Zotero\\storage\\TW6NDTAS\\Verbruggen et al. - 2014 - Banishing the Control Homunculi in Studies of Acti.pdf:application/pdf},
}

@article{weigard_role_2017,
